[user]
    name = Teo Wei Shen
    email = 7098637+tws4793@users.noreply.github.com

[alias]
    alias = ! git config --get-regexp ^alias\\. | sed -e s/^alias\\.// -e s/\\ /\\ =\\ /
    a = add
    aa = add .
    ap = add --patch
    b = branch
    c = commit
    cl = clone
    cm = commit -m
    co = checkout
    cu = commit --amend --no-edit
    d = diff
    f = fetch --all --verbose
	ignore = "!gi() { curl -sL https://www.toptal.com/developers/gitignore/api/$@ ;}; gi"
    lg = log --graph --oneline
    l1 = log -1
    m = merge
    nope = checkout .
    ps = push
    pu = pull
    r = remote
    s = status
    t = tag
    wait = reset HEAD
    uc = reset --soft HEAD^

[init]
    defaultBranch = main

[core]
    editor = vim

[color]
    ui = true

[pull]
    rebase = true

[rebase]
    autoStash = true
